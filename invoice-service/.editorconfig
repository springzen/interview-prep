# .editorconfig (place in project root)
root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
max_line_length = 120
ij_java_blank_lines_after_imports = 1
ij_java_imports_layout = *,javax.**,java.**,org.**,com.**